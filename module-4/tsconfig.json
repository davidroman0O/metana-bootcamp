{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
		"baseUrl": "./",
		"moduleResolution": "node",
		"paths": {
			"@openzeppelin/*": ["node_modules/@openzeppelin/*"]
		}
  }
}
